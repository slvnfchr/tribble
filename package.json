{
  "name": "tribble",
  "version": "0.0.3",
  "description": "Boilerplate for static & microservices-based web applications",
  "homepage": "https://github.com/slvnfchr/tribble",
  "bin": "bin/tribble.js",
  "scripts": {
    "test": "mocha --reporter spec test/*.js",
    "lint:bin": "$(npm bin)/eslint ./bin; exit 0",
    "lint:lib": "$(npm bin)/eslint ./lib; exit 0",
    "lint:test": "$(npm bin)/eslint ./test; exit 0",
    "lint": "npm run lint:bin && npm run lint:lib && npm run lint:test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slvnfchr/tribble.git"
  },
  "keywords": [
    "requirejs",
    "modules",
    "amd",
    "bundle"
  ],
  "author": "Sylvain Foucher",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^2.16.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.14.0",
    "mime-types": "^2.1.12",
    "minimist": "^1.2.0",
    "tribble-plugin": "latest"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  }
}
